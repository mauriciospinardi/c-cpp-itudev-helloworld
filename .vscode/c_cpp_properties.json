{
   "env": {
      "toolsFolder": "C:/cygwin64"
   },
   "configurations": [
      {
         "name": "gtk-hello-world",
         "compilerPath": "${toolsFolder}/bin",
         "intelliSenseMode": "gcc-x64",
         "includePath": [
            "${toolsFolder}/**",
            "${workspaceFolder}/**"
         ],
         "defines": [
            "_DEBUG_",
            "_PROJECT_NAME_=\"packager\"",
            "_PROJECT_VERSION_=\"VV.vv.bb\""
         ],
         "cStandard": "c99",
         "cppStandard": "c++11",
         "browse": {
            "limitSymbolsToIncludedHeaders": true
         }
      }
   ],
   "version": 4
}
